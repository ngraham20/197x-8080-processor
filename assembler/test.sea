BEGIN

# start

copi a0 2
# a0 = 2

copi b0 2
# b0 = 2

copi b1 24
# b1 = 24

copi b2 1
# b2 = 1

copi b3 20
# b3 = 20

copi c0 0
# c0 = 0

addi b0 4
# b0 = 2 + 4 = 6

:Loop
add c0 c0 b0
# c0 = c0 + 6

seq c1 c0 b1
# c1 = 1 IF c0 = 24

fjmp c1 Loop
# Jump IF c1 != 1

copy a0 c0
# a0 = c0 = 24

subi a0 4
# a0 = 24 - 4 = 20

sub c0 c0 a0
# c0 = 24 - 20 = 4

seq c2 c0 b3
# c2 = 1 IF c0 = 20

tjmp c2 Skip
# Jump IF c2 = 1

addi a0 1
# a0 = 20 + 1 [skipped]

:Skip
xor a1 a0 b0
# a1 = a0 XOR b0 = ...00010100 XOR ...00000110 = ...00010010 = 18d

srl a1 b2
# a1 = ...00100100 = 36d
